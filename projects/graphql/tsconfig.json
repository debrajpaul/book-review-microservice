{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "ES2020",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@abstractions/*": ["../commons/abstractions/src/*"],
      "@queue/*": ["../commons/queue/src/*"],
      "@stores/*": ["../commons/stores/src/*"],
      "@utils/*": ["../commons/utils/src/*"],
      "@validation/*": ["../commons/validation/src/*"],
      "@services/*": ["../commons/services/src/*"]
    }
  },
  "include": ["src", "../commons/*"],
  "exclude": ["node_modules", "./dist"]
}
